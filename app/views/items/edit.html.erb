<h1>Edit Your Item</h1>


   <div class="col-md-8">
     <%= form_for @a2 do |f| %>
       <div class="form-group">
         <%= f.label :name %>
         <%= f.text_field :name, class: 'form-control', placeholder: "Enter item name" %>
       </div>
  
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
     
     

   </div>






